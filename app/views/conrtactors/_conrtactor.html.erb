<div id="<%= dom_id conrtactor %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Type:</strong>
    <%= conrtactor.type %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= conrtactor.name %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Fullname:</strong>
    <%= conrtactor.fullname %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Email:</strong>
    <%= conrtactor.email %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Manager:</strong>
    <%= conrtactor.manager %>
  </p>

  <% if action_name != "show" %>
    <%= link_to "Show this conrtactor", conrtactor, class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%= link_to "Edit this conrtactor", edit_conrtactor_path(conrtactor), class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium" %>
    <hr class="mt-6">
  <% end %>
</div>
